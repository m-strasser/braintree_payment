    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^8.3.2",
    "parcel-bundler": "^1.12.3",
    "sinon": "^10.0.0"
  },
  "dependencies": {}
